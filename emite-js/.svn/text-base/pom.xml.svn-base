<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.calclab</groupId>
  <artifactId>emite-js</artifactId>
  <packaging>jar</packaging>
  <version>0.4.0</version>
  <description>emite javascript bridge</description>
  <name>emite javascript</name>
  <url>http://emite.googlecode.com</url>
  <dependencies>
    <dependency>
      <groupId>com.google.gwt</groupId>
      <artifactId>gwt-user</artifactId>
      <version>${gwt.version}</version>
    </dependency>
    <dependency>
      <groupId>com.calclab</groupId>
      <artifactId>suco</artifactId>
      <version>0.5.2.2</version>
    </dependency>
    <dependency>
      <groupId>com.calclab</groupId>
      <artifactId>emite</artifactId>
      <classifier>core</classifier>
      <version>0.4.6.2</version>
      <type>jar</type>
    </dependency>
    <dependency>
      <groupId>com.calclab</groupId>
      <artifactId>emite</artifactId>
      <classifier>browser</classifier>
      <version>0.4.6.1</version>
      <type>jar</type>
    </dependency>
    <dependency>
      <groupId>org.timepedia.exporter</groupId>
      <artifactId>gwt-exporter</artifactId>
      <type>jar</type>
      <version>2.0-alpha</version>
    </dependency>
    <dependency>
      <groupId>com.allen_sauer.gwt.log.gwt-log
      </groupId>
      <artifactId>gwt-log</artifactId>
      <version>2.5.2</version>
    </dependency>
  </dependencies>
  <pluginRepositories>
    <pluginRepository>
      <id>kune-maven</id>
      <url>http://kune.ourproject.org/mavenrepo/
      </url>
    </pluginRepository>
    <pluginRepository>
      <id>gwt-maven</id>
      <url> http://gwt-maven.googlecode.com/svn/trunk/mavenrepo/</url>
    </pluginRepository>
  </pluginRepositories>
  <repositories>
    <repository>
      <id>gwt-maven</id>
      <url> http://gwt-maven.googlecode.com/svn/trunk/mavenrepo/</url>
    </repository>
    <repository>
      <id>kune-maven</id>
      <url>http://kune.ourproject.org/mavenrepo/
      </url>
    </repository>
    <repository>
      <id>gwtext</id>
      <url>http://www.gwt-ext.com/maven2/
      </url>
    </repository>
  </repositories>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
      <plugin>
        <groupId>com.totsp.gwt</groupId>
        <artifactId>maven-googlewebtoolkit2-plugin
        </artifactId>
        <version>2.0-beta23</version>
        <configuration>
          <style>${gwt.style}</style>
          <runTarget>com.calclab.emite.js.EmiteJS/LogDemo.html?log_level=DEBUG</runTarget>
          <compileTargets>
            <compileTarget>com.calclab.emite.js.EmiteJS</compileTarget>
          </compileTargets>
          <generatorRootClasses>
          </generatorRootClasses>
          <generatorDestinationPackage>
          </generatorDestinationPackage>
          <generateGettersAndSetters>false</generateGettersAndSetters>
          <generatePropertyChangeSupport>true
          </generatePropertyChangeSupport>
        </configuration>
        <executions>
          <execution>
            <goals>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <profiles>
    <profile>
      <id>gwt</id>
      <activation>
        <jdk>1</jdk>
      </activation>
      <properties>
        <gwt.version>1.5.3</gwt.version>
        <gwt.style>PRETTY</gwt.style>
      </properties>
    </profile>
    <profile>
      <id>production</id>
      <properties>
        <gwt.style>OBF</gwt.style>
        <gwt.version>1.5.3</gwt.version>
      </properties>
    </profile>
  </profiles>
</project>
